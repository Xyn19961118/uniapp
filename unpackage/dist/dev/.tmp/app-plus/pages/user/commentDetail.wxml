<view class="container"><view class="top"><image class="pic" src="{{dataObj.users.headimg}}" mode="aspectFill" lazy-load></image><view class="info"><view class="name">{{dataObj.users.user_name}}</view><view class="stars"><block wx:for="{{dataObj.grade}}" wx:for-item="item" wx:for-index="index" wx:key="index"><image class="star" src="http://m.prizemart.cn/mobile/images/star-active.png"></image></block></view></view><view class="time">{{dataObj.create_time}}</view></view><view class="content">{{''+dataObj.content+''}}</view><view class="imgs"><block wx:for="{{urls}}" wx:for-item="item" wx:for-index="index" wx:key="index"><image class="img" src="{{item}}" data-src="{{item}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" bindtap="__e"></image></block></view></view>