<view class="shopcar-content data-v-f06a7fc6"><view class="shopcar-title data-v-f06a7fc6"><image class="shopcar-head-logo data-v-f06a7fc6" src="{{item.shop.shop_img}}" lazy-load></image><view class="shopcar-head-name data-v-f06a7fc6">{{item.shop.shop_name}}</view><view class="shopcar-head-right data-v-f06a7fc6"><block wx:if="{{isHaveBonus}}"><view class="shopcar-head-receive data-v-f06a7fc6" data-id="{{item.shop_id}}" data-name="{{item.shop.shop_name}}" data-event-opts="{{[['tap',[['handleReceive',['$event']]]]]}}" bindtap="__e">领券</view></block><view data-event-opts="{{[['tap',[['handleOver',['$event']]]]]}}" class="shopcar-head-handle data-v-f06a7fc6" bindtap="__e">{{isOver?"完成":"编辑"}}</view></view></view><view class="shopcar-items data-v-f06a7fc6"><block wx:for="{{item.goodsList}}" wx:for-item="goods" wx:for-index="index" wx:key="index"><view class="shopcar-item data-v-f06a7fc6"><item vue-id="{{'1-'+index}}" goods="{{goods}}" isOver="{{isOver}}" data-event-opts="{{[['^getNumber',[['getNumber']]],['^changeStatus',[['changeStatus']]],['^delGoods',[['delGoods']]],['^getNumberId',[['getNumberId']]]]}}" bind:getNumber="__e" bind:changeStatus="__e" bind:delGoods="__e" bind:getNumberId="__e" class="data-v-f06a7fc6" bind:__l="__l"></item></view></block></view></view>