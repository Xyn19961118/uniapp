<view class="{{['goo-container data-v-3ab9eba3',item.isShow?'aa':'bb']}}"><view class="goo-top data-v-3ab9eba3" data-index="{{index}}" data-event-opts="{{[['tap',[['handleShow',['$event']]]]]}}" catchtap="__e"><view class="goo-item goo-item01 data-v-3ab9eba3">{{item.goods_name}}</view><view class="goo-item goo-item02 data-v-3ab9eba3">{{"￥"+item.shop_price}}</view><view class="goo-item goo-item03 data-v-3ab9eba3">{{"￥"+item.supply_price}}</view><view class="goo-item goo-item04 data-v-3ab9eba3">￥<view class="goo-item-price data-v-3ab9eba3">{{item.new_shop_price}}</view></view><view class="goo-item goo-item05 data-v-3ab9eba3"><view class="goo-handle-change data-v-3ab9eba3" data-id="{{item.goods_id}}" data-val="{{item.shop_price}}" data-event-opts="{{[['tap',[['handelChange',['$event']]]]]}}" catchtap="__e">修改</view></view></view><view class="{{['goo-content-wrap data-v-3ab9eba3',item.is_show?'goo-show':'']}}"><view class="goo-content data-v-3ab9eba3"><view class="goo-info data-v-3ab9eba3"><view class="goo-pic data-v-3ab9eba3"><v-pic vue-id="1" img="{{item.goods_img}}" class="data-v-3ab9eba3" bind:__l="__l"></v-pic></view><view class="goo-cont data-v-3ab9eba3"><view class="goo-cont-name data-v-3ab9eba3">{{item.goods_name}}</view><view class="goo-cont-items data-v-3ab9eba3"><view class="goo-cont-item data-v-3ab9eba3">{{"品牌："+item.brand_name}}</view><view class="goo-cont-item data-v-3ab9eba3">{{"货号："+item.goods_sn}}</view><view class="goo-cont-item data-v-3ab9eba3">{{"规格："+item.goods_rate}}</view><view class="goo-cont-item data-v-3ab9eba3">{{"原产地："+item.country_name}}</view><view class="goo-cont-item data-v-3ab9eba3">{{"保质期："+item.life_time}}</view></view><view class="goo-cont-oth data-v-3ab9eba3">本产品折扣为：<view class="goo-red data-v-3ab9eba3">{{item.goods_rate+"%"}}</view>,盈利：<view class="goo-red data-v-3ab9eba3">{{"¥"+item.profit_price}}</view></view></view></view><view class="goo-btns data-v-3ab9eba3"><view class="goo-btn data-v-3ab9eba3" data-id="{{item.goods_id}}" data-event-opts="{{[['tap',[['changeTop',['$event']]]]]}}" bindtap="__e">{{item.shopGoodsStatus.is_top?'已置顶':'置顶'}}</view><view class="goo-btn data-v-3ab9eba3" data-id="{{item.goods_id}}" data-event-opts="{{[['tap',[['changeHots',['$event']]]]]}}" bindtap="__e">{{item.shopGoodsStatus.is_hots?'已推荐':'推荐'}}</view></view></view></view></view>